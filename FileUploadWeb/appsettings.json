{
  "ConnectionStrings": {
    "DiscordDbConnection": "DataSource=users.db;Cache=Shared",
    "FilesDbConnection": "DataSource=files.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "DefaultScheme": "Discord",
    "ClientId": "[YOUR DISCORD CLIENT ID]",
    "ClientSecret": "[YOUR DISCORD CLIENT SECRET]",
    "RedirectUri": "[YOUR WEBSITE URL]/DiscordLoginCallback",
    "AllowedServerId": "[YOUR SERVER ID]"
  },
  "AppSettings": {
    "StoredFilesPath": "/app/files",
    "FileSizeLimit": 3221225472
  },
  "DiscordBot": {
    "Token": "[YOUR DISCORD BOT TOKEN]",
    "Prefix": ";"
  }
}
